{
    "ServerProxy": {
        "servers": {
            "code-server": {
                "command": [
                    "code-server",
                    "--no-auth",
                    "--disable-telemetry",
                    "--allow-http",
                    "--port={port}"
                ],
                "timeout": 20,
                "launcher_entry": {
                    "title": "VS Code",
                    "enabled" : false
                }
            },
            "rstudio": {
                "command": [
                    "rserver",
                    "--www-port={port}",
                    "--www-frame-origin=same",
                    "--www-address=127.0.0.1"
                ],
                "timeout": 20,
                "launcher_entry": {
                        "title": "RStudio",
                        "enabled" : false
                }
            }
        }
    }
}