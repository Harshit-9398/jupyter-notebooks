{
    "ServerProxy": {
        "servers": {
            "VSCode": {
                "command": [
                    "code-server",
                    "--auth=none",
                    "--disable-telemetry",
                    "--port={port}"
                ],
                "timeout": 20,
                "launcher_entry": {
                    "title": "VS Code",
                    "enabled" : false
                }
            },
            "rstudio": {
                "command": [
                    "rserver",
                    "--www-port={port}",
                    "--www-frame-origin=same",
                    "--user={user}",
                    "--auth-none=1",
                    "--server-daemonize=0",
                    "--server-working-dir=/opt/app-root/src"
                ],
                "timeout": 20,
                "absolute_url": false,
                "launcher_entry": {
                        "title": "RStudio",
                        "enabled" : false
                }
            }
        }
    }
}